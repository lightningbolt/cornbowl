<table class="content">
  <tr>
    <th></th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @cornbowlers.each do |cornbowler| %>
  <tr>
    <td><%= cornbowler.name %></td>
    <td><%= link_to 'Show', cornbowler %></td>
    <td><%= link_to 'Edit', edit_cornbowler_path(cornbowler) %></td>
    <td><%= link_to 'Destroy', cornbowler, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

